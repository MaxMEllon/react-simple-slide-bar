{
  "name": "react-simple-slider",
  "version": "v0.1.2",
  "main": "lib/index.js",
  "description": "simple slider component for React.js",
  "keywords": [
    "react",
    "react-component",
    "slider",
    "component"
  ],
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "babel src/components/Slider.js -o lib/index.js",
    "prepublish": "npm run build",
    "predev": "webpack",
    "dev": "abby dev:* --parse parallel",
    "dev:server": "node ./webpack.dev.server.js",
    "dev:open": "wait-on http://localhost:8080 && opener http://localhost:8080/"
  },
  "abigail": {
    "plugins": {
      "watch": false,
      "parse": "raw",
      "launch": "force"
    }
  },
  "devDependencies": {
    "abigail": "^1.9.1",
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-power-assert": "^1.0.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-1": "^6.13.0",
    "css-loader": "^0.23.1",
    "debug": "^2.2.0",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "isparta-loader": "^2.0.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.14.2",
    "node-sass": "^3.8.0",
    "pascal-case": "^2.0.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-hot-loader": "^1.3.0",
    "react-tap-event-plugin": "^1.0.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "wait-on": "^1.5.2",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "react-addons-deep-compare": "^0.0.1"
  }, 
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0"
  }
}
